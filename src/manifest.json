{
  "name": "MDN VS-Code Theme",
  "description": "Styles MDN code blocks like the default vscode theme.",
  "version": "1.1",
  "icons": { "128": "icon.png" },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*", "https://interactive-examples.mdn.mozilla.net/*"],
      "css": ["styles.css"],
      "all_frames": true
    }
  ]
}
